{
  "name": "thinx-console",
  "version": "1.0.0",
  "description": "THiNX Console",
  "main": "gulpfile.js",
  "scripts": {
    "preinstall": "npm install --ignore-scripts --no-audit",
    "test": "protractor test/conf.js",
    "coverage": "npm test; nyc report --reporter=text-lcov | coveralls",
    "build": "VERSION_CODE=$(git rev-parse --short HEAD) ./node_modules/.bin/gulp dev",
    "build:test": "VERSION_CODE=$(git rev-parse --short HEAD) COMPOSE_PROJECT_NAME=test LANDING_HOSTNAME=https://thinx.cloud API_BASEURL=https://rtm.thinx.cloud API_HOSTNAME=https://rtm.thinx.cloud/api WEB_HOSTNAME=https://rtm.thinx.cloud ./node_modules/.bin/gulp dev",
    "buildAll": "./node_modules/.bin/gulp prod",
    "lint": "./node_modules/.bin/eslint --ignore-path .eslintignore ./",
    "cy:install": "./node_modules/cypress/bin/cypress install",
    "start": "http-server ./html -p 8080 -g",
    "cy:run": "./node_modules/cypress/bin/cypress run",
    "cy": "start-test 'npm run start' 8080 'npm run cy:run'",
    "cy:open": "./node_modules/cypress/bin/cypress open"
  },
  "author": "Mikulas Sychra @ THiNX Cloud",
  "repository": {
    "type": "git",
    "url": "git://github.com/suculent/thinx-console.git"
  },
  "license": "ISC",
  "dependencies": {
    "negotiator": "0.6.3",
    "qs": "6.10.3"
  },
  "resolutions": {
    "graceful-fs": "4.2.10",
    "negotiator": "0.6.3",
    "lodash": "4.17.21",
    "lodash.template": "4.5.0",
    "postcss": "8.2.13",
    "uuid": "8.3.2",
    "minimatch": "3.0.4",
    "glob-parent": "5.1.2"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.12",
    "coveralls": "^3.1.1",
    "cypress": "^9.4.1",
    "eslint": "^8.7.0",
    "eslint-config-angular": "^0.5.0",
    "eslint-config-jquery": "^3.0.0",
    "eslint-plugin-angular": "^4.1.0",
    "eslint-plugin-jquery": "^1.5.1",
    "gulp": "^3.9.1",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-connect": "^5.7.0",
    "gulp-debug": "3.2.0",
    "gulp-ignore": "^3.0.0",
    "gulp-inject-envs": "^1.1.1",
    "gulp-noop": "^1.0.1",
    "gulp-prettify": "^0.5.0",
    "gulp-remove-code": "^3.0.4",
    "gulp-rename": "^2.0.0",
    "gulp-rtlcss": "^1.4.2",
    "gulp-sass": "^5.1.0",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify-es": "^3.0.0",
    "gulp-util": "^3.0.8",
    "http-server": "^14.1.0",
    "merge-stream": "^2.0.0",
    "nyc": "^15.1.0",
    "start-server-and-test": "^1.14.0"
  },
  "nyc": {
    "check-coverage": true,
    "include": [
      "src/**/*.js"
    ],
    "exclude": [
      "cypress/**/*.spec.js"
    ],
    "reporter": [
      "lcov",
      "text-summary"
    ],
    "cache": true,
    "all": true,
    "report-dir": "./coverage"
  }
}
